curl --location --request POST 'http://localhost:8083/connectors/' \
--header 'Content-Type: application/json' \
--data-raw '{
  "name": "test_connector_ten",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSourceConnector",
    "connection.url": "jdbc:oracle:thin:@172.20.45.104:1521/orcl",
    "connection.user": "grc_test",
    "connection.password": "grc_test",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "topic.prefix": "incre_test",
    "mode": "timestamp",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "query": "SELECT * FROM DG_ACCOUNTING.ADD_CREDIT_INFO",
    "value.converter.schema.registry.url": "http://schema-registry:8081",
    "key.converter.schema.registry.url": "http://schema-registry:8081",
    "offset.flush.timeout.ms": 2000
  }
}'


